    "optimist": "~0.6.1",
    "xregexp": "~2.0.0"
  },
  "devDependencies": {
    "coffee-script-redux": "2.0.0-beta7",
    "commonjs-everywhere": "0.9.x",
    "deep-equal": "~0.2.1",
